<h2 class="heading"> All Books </h2>

<div class="next-page">
  <%= will_paginate %>
</div>
<br>
<ul class="flex-container">
  <% @books.each do |book| %>
  <li class="book_box"><%= link_to book.name, book_path(book)%></li>
  <br>
  <% end %>
</ul>
<div class="new-book">
<%= link_to "Add New Book", new_book_path %>
</div>
